{
	"name": "ninjaparade/content",
	"type": "platform-extension",
	"description": "content creation for website",
	"authors": [
		{
			"name": "Ninjaparade",
			"email": "ninja@ninjaparade.ca"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cartalyst/composer-installers": "~1.2",
		"illuminate/support": "4.2.*"
	},
	"autoload": {
		"classmap": [
			"database/migrations",
			"database/seeds"
		],
		"psr-4": {
			"Ninjaparade\\Content\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
